<html>
    <body>
        <h1>Lighthouse Film Lab</h1>
        <p>Welcome to LFL!</p>
        <h2>Users</h2>
        <ul>
            #for(user in users) {
                <li>#(user.name) #(user.surName) (#(user.username))</li>
            }
        </ul>
        <br>
        <h2>Orders</h2>
        <ul>
            #for(order in orders) {
                <li>Order #(order.orderNumber)</li>
            }
        </ul>
    </body>
</html>